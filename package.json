{
  "name": "salo-cms-ai",
  "type": "module",
  "packageManager": "bun@1.1.32",
  "private": true,
  "keywords": [
    "template",
    "biomejs",
    "bun",
    "typescript",
    "tailwindcss",
    "astrojs",
    "cloudflare",
    "react",
    "framer-motion",
    "shadcn"
  ],
  "author": {
    "email": "devopsick@pm.me",
    "name": "Hrihorii Ilin - @Frikadellios",
    "url": "https://github.com/Frikadellios"
  },
  "license": "Apache-2.0",
  "description": "I am from Ukraine :D",
  "repository": {
    "type": "git",
    "url": "https://github.com/Frikadellios/salo-cms-ai.git"
  },
  "bugs": {
    "url": "https://github.com/Frikadellios/salo-cms-ai/issues"
  },
  "homepage": "https://github.com/Frikadellios/salo-cms-ai/readme",
  "engines": {
    "node": "22.10.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-checker -exclude \"**/node_modules/**\"",
    "browserslist": "bunx update-browserslist-db@latest",
    "tailwindcss": "bunx @tailwindcss/cli@next -i ./src/styles/app.css -o ./src/styles/index.css",
    "dev": "bun run tailwindcss -m && bunx --bun astro dev --verbose",
    "start": "bun run cf-typegen && bun run astro check --verbose && bun run tailwindcss -m && bunx --bun astro dev --verbose",
    "build": "bunx --bun astro build --verbose ./dist",
    "preview": "bunx --bun astro build -- --force ./dist && bun run wrangler pages dev ./dist",
    "astro": "astro",
    "deploy": "bun run build && bun run wrangler pages deploy ./dist",
    "inspect": "bunx serve .vite-inspect",
    "cf-typegen": "wrangler types",
    "lint": "conc bun:lint:*",
    "lint:format": "bunx @biomejs/biome format . --write",
    "lint:fix": "bunx @biomejs/biome check . --fix",
    "lint:check": "bunx @biomejs/biome check ./* --write --unsafe",
    "typesafe-i18n": "typesafe-i18n"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "@types/js-cookie": "^3.0.6",
    "@types/mdx": "latest",
    "@types/node": "latest",
    "@types/web": "latest",
    "@vitejs/plugin-legacy": "^5.4.2",
    "autoprefixer": "^10.4.20",
    "bun-devtools": "latest",
    "bun-types": "latest",
    "editorconfig-checker": "^6.0.0",
    "postcss": "^8.4.47",
    "postcss-discard-comments": "^7.0.3",
    "postcss-lightningcss": "^1.0.1",
    "postcss-preset-env": "^10.0.8",
    "terser": "^5.36.0",
    "vite-plugin-inspect": "^0.8.7",
    "vite-plugin-lightningcss": "^0.0.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^11.1.0",
    "@astrojs/mdx": "^3.1.8",
    "@astrojs/partytown": "^2.1.2",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.9",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/ts-plugin": "^1.10.3",
    "@headlessui/react": "^2.1.10",
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@tailwindcss/cli": "^4.0.0-alpha.29",
