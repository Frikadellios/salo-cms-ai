    "@tailwindcss/postcss": "^4.0.0-alpha.29",
    "@tailwindcss/vite": "^4.0.0-alpha.29",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "astro": "^4.16.7",
    "balance-text": "^3.3.1",
    "caniuse-lite": "^1.0.30001669",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "concurrently": "^9.0.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.9",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.453.0",
    "motion": "^10.18.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-router-dom": "^6.27.0",
    "reading-time": "^1.5.0",
